.users-index-background
  .users-index-box
    %h1.users-index-title ユーザー 一覧
    .user-search-box
      = search_form_for @q , url: users_path do |f|
        %div
          %span= f.label :名前
          = f.search_field :name_cont
          = f.submit "検索"
    #js-user-list
      = render partial: 'index', locals: { user: @users }
    = link_to_next_page @users, 'もっと見る', remote: true, id: 'js-more-link'  
        
        

